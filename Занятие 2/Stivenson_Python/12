BP = 0.00
CP = 14.00
AP = 23.00
SP = 18.00
BL = 2
CL = 12
AL = 64
t = 0
l = input("Введите возраст посетителя (пустая строка для окончания ввода): ")
while l != "":
    a = int(l)
    if a <= BL:
        t = t + BP
    elif a <= CL:
        t = t + CP
    elif a <= AL:
        t = t + AP
    else:
        t = t + SP
l = input("Введите возраст посетителя (пустая строка для окончания ввода): ")
print("Сумма посещения зоопарка для этой группы составит $%.2f" % t)
