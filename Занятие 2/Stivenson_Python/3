c4 = 261.63
d4 = 293.66
e4 = 329.63
f4 = 349.23
g4 = 392.00
a4 = 440.00
b4 = 493.88
name = input("Введите название ноты в виде буквы и цифры, например C4: ")
n = name[0]
o = int(name[1])
if n == "C":
    f = c4
elif n == "D":
    f = d4
elif n == "E":
    f = e4
elif n == "F":
    f = f4
elif n == "G":
    f = g4
elif n == "A":
    f = a4
elif n == "B":
    f = b4
f = f / 2 ** (4 - o)
print("Частота ноты", name, "равна", f)
