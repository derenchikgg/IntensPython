y = int(input("Введите год: "))
if y % 12 == 8:
    a = "Дракон"
elif y % 12 == 9:
    a = "Змея"
elif y % 12 == 10:
    a = "Лошадь"
elif y % 12 == 11:
    a = "Коза"
elif y % 12 == 0:
    a = "Обезьяна"
elif y % 12 == 1:
    a = "Петух"
elif y % 12 == 2:
    a = "Собака"
elif y % 12 == 3:
    a = "Свинья"
elif y % 12 == 4:
    a = "Крыса"
elif y % 12 == 5:
    a = "Бык"
elif y % 12 == 6:
    a = "Тигр"
elif y % 12 == 7:
    a = "Кролик"
print("Год %d ассоциирован с животным: %s." % (y, a))
