a = int(input("Введите число a: "))
b = int(input("Введите число b: "))
c = int(input("Введите число c: "))
if (a % 2 == 0 and b % 2 != 0) or (a % 2 != 0 and b % 2 == 0):
    print("Да")
elif (a % 2 == 0 and c % 2 != 0) or (a % 2 != 0 and c % 2 == 0):
    print("Да")
elif (b % 2 == 0 and c % 2 != 0) or (b % 2 != 0 and c % 2 == 0):
    print("Да")
else:
    print("Нет")
